---
layout: base.njk
title: #{{ tag  }}
tags:
  - tag
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - post
    - tag
permalink: /tag/{{ tag }}/
---
<main id="content">
  <h1>#{{ tag }}</h1>

  <ul>
    {% set taglist = collections[tag] %}
    {% for post in taglist | reverse %}
      <li>
        <a href="{{ post.url }}">{{ post.data.title }}</a>
      </li>
    {% endfor %}
  </ul>
</main>